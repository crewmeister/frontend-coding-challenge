{
  "rulesDirectory": [
    "node_modules/@nrwl/workspace/src/tslint",
    "node_modules/codelyzer"
  ],
  "rules": {
    "prettier": true,
    "adjacent-overload-signatures": true,
    "arrow-return-shorthand": true,
    "ban-types": [true, ["Object", "Use {} instead."], ["String"]],
    "callable-types": true,
    "class-name": true,
    "comment-format": [true, "check-space", "check-uppercase", "allow-trailing-lowercase"],
    "curly": true,
    "cyclomatic-complexity": [true, 20],
    "deprecation": true,
    "eofline": true,
    "file-name-casing": [true, "kebab-case"],
    "forin": true,
    "import-blacklist": [true, "rxjs/Rx"],
    "import-spacing": true,
    "interface-over-type-literal": true,
    "interface-name": false,
    "label-position": true,
    "max-classes-per-file": [true, 10, "exclude-class-expressions"],
    "max-file-line-count": [true, 1000],
    "member-access": true,
    "member-ordering": [
      true,
      {
        "order": [
          "public-static-field",
          "protected-static-field",
          "private-static-field",
          "public-instance-field",
          "protected-instance-field",
          "private-instance-field",
          "public-static-method",
          "protected-static-method",
          "private-static-method",
          "constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "new-parens": true,
    "newline-before-return": true,
    "no-arg": true,
    "no-async-without-await": true,
    "no-bitwise": true,
    "no-console": [
      true,
      "info",
      "log",
      "warn",
      "time",
      "timeEnd",
      "trace",
      "error"
    ],
    "no-construct": true,
    "no-consecutive-blank-lines": [true, 2],
    "no-debugger": true,
    "no-default-export": true,
    "no-duplicate-imports": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-empty-interface": true,
    "no-eval": true,
    "no-floating-promises": false,
    "no-inferrable-types": [true, "ignore-params"],
    "no-irregular-whitespace": true,
    "no-misused-new": true,
    "no-non-null-assertion": true,
    "no-object-literal-type-assertion": true,
    "no-reference": true,
    "no-require-imports": true,
    "no-restricted-globals": [true, "name", "length", "event"],
    "no-shadowed-variable": true,
    "no-sparse-arrays": true,
    "no-string-literal": true,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unbound-method": [
      true,
      {
        "ignore-static": true,
        "whitelist": ["expect"],
        "allow-typeof": true
      }
    ],
    "no-unnecessary-class": [true, "allow-empty-class", "allow-constructor-only"],
    "no-unnecessary-initializer": true,
    "no-unsafe-any": false,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-unused-variable": false,
    "no-var-keyword": true,
    "no-void-expression": true,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-finally",
      "check-whitespace"
    ],
    "ordered-imports": true,
    "prefer-const": true,
    "prefer-conditional-expression": true,
    "prefer-object-spread": true,
    "radix": true,
    "restrict-plus-operands": true,
    "space-within-parens": true,
    "static-this": true,
    "switch-default": true,
    "triple-equals": [true, "allow-null-check"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "unified-signatures": true,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case",
        "require-const-for-all-caps"
      ]
    },
    "whitespace": [true,
      "check-branch",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-type-operator",
      "check-preblock"],
    "no-any": true,
    "no-host-metadata-property": true,
    "no-input-rename": true,
    "no-inputs-metadata-property": true,
    "no-magic-numbers": true,
    "no-output-rename": true,
    "no-outputs-metadata-property": true,
    "no-tautology-expression": true,
    "use-default-type-parameter": true,
    "use-isnan": true,
    "use-life-cycle-interface": true,
    "unnecessary-constructor": true,
    "use-pipe-transform-interface": true,
    "component-class-suffix": true,
    "directive-class-suffix": true,
    "template-use-track-by-function": true,
    "type-literal-delimiter": true,
    "prefer-inline-decorator": true,
    "component-max-inline-declarations": [true, {"animations": 15, "styles": 10, "template": 10}],
    "no-input-prefix": [true, "can", "is", "should"],
    "no-unused-css": true,
    "relative-url-prefix": true,
    "template-conditional-complexity": true,
    "template-no-call-expression": false,
    "use-pipe-decorator": true,
    "import-destructuring-spacing": true,
    "pipe-prefix": [true, "dpsfui"],
    "no-import-side-effect": false,
    "no-var-requires": true,
    "only-arrow-functions": [true, "allow-declarations", "allow-named-functions"],
    "typedef": [false, "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"],
    "function-constructor": true,
    "no-dynamic-delete": true,
    "no-for-in-array": true,
    "no-implicit-dependencies": false,
    "no-inferred-empty-object-type": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-promise-as-boolean": true,
    "no-this-assignment": true,
    "strict-boolean-expressions": false,
    "strict-comparisons": true,
    "strict-string-expressions": true,
    "strict-type-predicates": true,
    "invalid-void": true,
    "no-mergeable-namespace": true,
    "prefer-readonly": true,
    "array-type": [true, "array-simple"],
    "binary-expression-operand-order": true,
    "no-boolean-literal-compare": true,
    "object-literal-key-quotes": [true, "consistent-as-needed"],
    "prefer-template": [true, "allow-single-concat"],
    "switch-final-break": [true, "always"],
    "unnecessary-bind": true,
    "arrow-parens": true,
    "number-literal-format": true
  }
}
